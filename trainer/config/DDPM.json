{
    "path": {
        "checkpoint": "./output/checkpoint",
        "log": "./output/log"
    },
    "modules": {
        "runner": "SingleGPU",
        "dataset": "FLAEP",
        "loader": "FLAEP",
        "model": "FLAEP",
        "loss": "FLAEP",
        "optimizer": "FLAEP",
        "viewer": "FLAEP"
    },
